data = [-0.225 -0.225	-0.225	-0.225	-0.225	0.45	0.45	0.525	0.525	0.6 1.35	0.3	1.35	0	1.5	0.075	01.05	0.15	1.425	0.2250]
    names= { 'Normais' 'Normais' 'Normais' 'Normais' 'Normais' 'Normais' 'Normais' 'Normais' 'Normais' 'Normais' 'Daltônico' 'Daltônico' 'Daltônico' 'Daltônico' 'Daltônico' 'Daltônico' 'Daltônico' 'Daltônico' 'Daltônico' 'Daltônico'}

boxplot(data,names)
title('Diagrama de Caixa')
xlabel('Respostas')
ylabel('L= log [(L1/L2)]')
