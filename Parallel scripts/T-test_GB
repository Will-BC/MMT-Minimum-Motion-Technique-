d=[-0.3 -0.3 -0.3 -0.225 -0.225 -0.225 -0.15 -0.15 -0.15 -0.075 0.975 0.975 0.975 0.975 0.975 1.05 1.05]; 
n=[-0.225 -0.225 -0.225	-0.15	0.075	0.075	0.075 0.075 0.375 0.375 0.375 0.45 0.45 0.525 0.6 0.6 0.6];

[h,p,ci,stats]=ttest(n,d)
